@using MISA.JETPAY.DemoPaymentGateway.Models;
@model DataNapas
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Cổng thanh toán JETPAY</title>
    <link href="/css/common.css" rel="stylesheet" />
    <link href="/css/input.css" rel="stylesheet" />
    <link href="/css/button.css" rel="stylesheet" />
    <script src="/lib/crawler.js"></script>
</head>
<body>
    <div class="main">
        <div class="g-row"></div>
        <div class="g-center">
            <div class="g-content">
                <div class="g-header">
                    <div class="g-banner"></div>
                </div>
                <div class="order">
                    <div class="order-left">
                        <div class="g-logo"></div>
                    </div>
                    <div class="order-center">
                        <fieldset class="order-info">
                            @{
                                var amount = Model.amount;
                                var orderId = Model.id;
                            }
                            <legend>Thông tin đơn hàng</legend>
                            <div class="g-same-row">
                                <div class="g-lable">Đơn hàng ::</div>
                                <div class="form-input"><input type="text" name="" readonly value="" id="txtDonHang" /></div>
                            </div>
                            <div class="g-same-row">
                                <div class="g-lable">Giá trị đơn hàng ::</div>
                                <div class="form-input"><input type="text" name="" readonly value="" id="txtGiaTriDonHang" /></div>
                            </div>
                        </fieldset>
                    </div>
                    <div class="order-right"></div>
                </div>
                <div class="g-line"></div>
                <!-- payment method -->
                <div class="payment-method">
                    @{
                        var result = Model.result;
                        var merchantId = Model.merchantId;
                        if (result == "SUCCESS")
                        {
                            <div style="text-align:center; height: 306px; width:100%; font-family:Tahoma; font-size:14px; padding-top:20px;">
                                <span class="font-bold">Giao dịch thanh toán thành công.</span><span id="orderResult" style="display:none;">@result</span><span id="merchantId" style="display:none;">@merchantId</span> <br />Cảm ơn quý khách đã sử dụng dịch vụ. Qúy khách sẽ được quay về trang của
                                Đơn vị chấp nhận thẻ để tiếp tục giao dịch.<br /> Trong trường hợp không chuyển được, Qúy khách vui lòng bấm <a style="color:blue; text-decoration:underline;" class="font-bold">VÀO ĐÂY</a> để biết thêm chi tiết
                            </div>

                        }
                        else
                        {
                            <div style="text-align:center; height: 306px; width:100%; font-family:Tahoma; font-size:14px; padding-top:20px;">
                                <span class="font-bold">Giao dịch thanh toán thất bại.</span><span id="orderResult" style="display:none;">@result</span><span id="merchantId" style="display:none;">@merchantId</span> <br />Cảm ơn quý khách đã sử dụng dịch vụ. Qúy khách sẽ được quay về trang của
                                Đơn vị chấp nhận thẻ để tiếp tục giao dịch.<br /> Trong trường hợp không chuyển được, Qúy khách vui lòng bấm <span style="color:blue; text-decoration:underline;" class="font-bold">VÀO ĐÂY</span> để biết thêm chi tiết
                            </div>
                        }
                    }
                </div>
                <!-- end payment method -->
                <div class="g-scroll">
                    <div id="bankscroll">
                        <img src="/content/img/VCB.png" alt="" />
                        <img src="/content/img/TCB.png" alt="" />
                        <img src="/content/img/VIB.png" alt="" />
                        <img src="/content/img/EIB.png" alt="" />
                        <img src="/content/img/CTG.png" alt="" />
                        <img src="/content/img/MB.png" alt="" />
                        <img src="/content/img/HDB.png" alt="" />
                        <img src="/content/img/ACB.png" alt="" />
                        <img src="/content/img/VAB.png" alt="" />
                        <img src="/content/img/STB.png" alt="" />
                        <img src="/content/img/MSB.png" alt="" />
                        <img src="/content/img/VPB.png" alt="" />
                        <img src="/content/img/DAB.png" alt="" />
                        <img src="/content/img/LPB.png" alt="" />
                        <img src="/content/img/TPB.png" alt="" />
                        <img src="/content/img/OCB.png" alt="" />
                        <img src="/content/img/OJB.png" alt="" />
                        <img src="/content/img/ABB.png" alt="" />
                        <img src="/content/img/NASB.png" alt="" />
                        <img src="/content/img/BVB.png" alt="" />
                        <img src="/content/img/GPB.png" alt="" />
                        <img src="/content/img/NCB.png" alt="" />
                        <img src="/content/img/VARB.png" alt="" />
                        <img src="/content/img/BIDV.png" alt="" />
                        <img src="/content/img/SEAB.png" alt="" />
                        <img src="/content/img/KLB.png" alt="" />
                    </div>
                </div>
                <div class="g-footer">
                    <div class="footer-center">Copyright © 2019 JETPAY, All rights reserved</div>
                    <div class="footer-right">
                        <img src="/content/img/trustVN.jpg" />
                        <img src="/content/img/verisign.jpg" />
                        <img src="/content/img/trustwave.jpg" />
                    </div>
                </div>
            </div>
        </div>
        <div class="g-row"></div>
    </div>
    <script src="/lib/jquery-3.5.1.min.js"></script>
    <script src="/js/common/common.js"></script>
    <script src="/js/napasresult.js"></script>
</body>
</html>